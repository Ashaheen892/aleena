   <script>
    // ======================= scroll to video ======================
    const posts = document.getElementById("posts");
    const reals = document.getElementById("reals");
    reals.style.display = "none"; // hide reals initially

    function scrollTotargetVideo(video) {
        const targetVideo = document.getElementById(video);
        posts.style.display = "none";
        reals.style.display = "block";
        targetVideo.scrollIntoView({ behavior: "auto" });
    }

    // ======================= close modal ======================
    function closeModal(event) {
        event.stopPropagation();
        reals.style.display = "none";
        posts.style.display = "flex";
    }
    // ======================= intersection observer for lazy loading videos ======================
    const observer = new IntersectionObserver((entries) => {
        entries.forEach((entry) => {
            const container = entry.target;
            const videoElement = container.querySelector('.video__player');
            const placeholder = container.querySelector('.placeholder');
            const videoSrc = container.dataset.video;

            if (entry.isIntersecting) {
                if (!videoElement.src) {
                    videoElement.src = videoSrc;
                    videoElement.classList.remove('hidden'); // Show video
                    placeholder?.remove(); // Remove placeholder if it exists
                }
                videoElement.play();
                videoElement.muted = false; // Unmute the video
            } else {
                videoElement.pause();
            }
        });
    }, { threshold: 0.5 });

    // Observing videos for lazy loading
    document.querySelectorAll('.video').forEach(video => observer.observe(video));
    // ======================= observer to pause first video on scroll out of view ======================
    const firstVideoContainer = document.getElementById('video-1');
    const firstVideo = firstVideoContainer.querySelector('.video__player');

    const pauseFirstVideoObserver = new IntersectionObserver((entries) => {
        entries.forEach((entry) => {
            if (!entry.isIntersecting) {
                firstVideo.pause();
            }
        });
    }, { threshold: 0.1 }); // Adjust threshold to detect when it's barely out of view

    pauseFirstVideoObserver.observe(firstVideoContainer);

    // ======================= pause/play on click ======================
    document.querySelectorAll('.video__player').forEach((video) => {

        const pauseBtn = video.nextElementSibling;

        video.addEventListener('click', () => {
            if (video.paused) {
                video.play();
                video.muted = false; // Ensure video is unmuted when playing
                pauseBtn.style.display = "none";
            } else {
                video.pause();
                pauseBtn.style.display = "flex";
            }
        });

        pauseBtn.addEventListener('click', () => {
            if (video.paused) {
                video.play();
                pauseBtn.style.display = "none";
            } else {
                video.pause();
                pauseBtn.style.display = "flex";
            }
        });
    });


</script>
